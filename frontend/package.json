{
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "dependencies": {
        "@welldone-software/why-did-you-render": "^3.2.1",
        "event-source-polyfill": "1.0.7",
        "forest-themes": "semantic-ui-forest/forest-themes#7f472cfbcc2093bb8c90cc56fba18ca8e3818028",
        "isomorphic-fetch": "^2.2.1",
        "native-base": "^2.12.1",
        "normalizr": "^3.4.0",
        "printj": "^1.2.1",
        "query-string": "^6.8.3",
        "re-reselect": "^3.3.0",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-dropzone": "^10.1.5",
        "react-image-crop": "^8.1.0",
        "react-native": "0.60.0",
        "react-native-gesture-handler": "^1.3.0",
        "react-navigation": "^3.11.0",
        "react-redux": "^7.1.0",
        "react-router": "^5.0.1",
        "react-router-dom": "^5.0.1",
        "react-scripts": "^3.0.1",
        "react-web-notification": "^0.7.0",
        "recharts": "^1.6.2",
        "redux": "^4.0.1",
        "redux-logger": "^3.0.6",
        "redux-thunk": "^2.3.0",
        "reselect": "^4.0.0",
        "semantic-ui-css": "^2.4.1",
        "semantic-ui-react": "^0.87.2"
    },
    "devDependencies": {
        "babel-jest": "24.8.0",
        "jest": "24.7.1",
        "metro-react-native-babel-preset": "0.55.0",
        "react-test-renderer": "16.8.6"
    },
    "jest": {
        "preset": "react-native"
    },
    "name": "AstroPhotoPlus_frontend",
    "private": true,
    "proxy": "http://localhost:5000",
    "scripts": {
        "build": "yarn run copy:assets && react-scripts build",
        "copy:assets": "yarn run copy:themes && yarn run remove_googlefonts_call",
        "copy:themes": "yarn run forest:clean && cp -a node_modules/forest-themes/dist/* public/themes/ && cp -a node_modules/semantic-ui-css/themes public/themes/bootswatch-v3/ && cp -a node_modules/semantic-ui-css/themes public/themes/bootswatch-v4/",
        "eject": "react-scripts eject",
        "forest:clean": "rm -rf public/themes; mkdir -p public/themes",
        "remove_googlefonts_call": "python3 scripts/remove_googlefonts_call.py",
        "run:android": "react-native run-android",
        "run:ios": "react-native run-ios",
        "start": "yarn run copy:assets && react-scripts start",
        "start:native": "node node_modules/react-native/local-cli/cli.js start",
        "test": "react-scripts test --env=jsdom",
        "test:native": "jest"
    },
    "version": "1.0.1"
}
